<div class="userinfo">
  <% if current_user.graffiti_image.present? %>
    <%= image_tag current_user.graffiti_image.url %>
  <% else %>
    <%= image_tag current_user.image %>
  <% end %>  
  <div class="usernamefame">
    <h3><%= current_user.username %></h3>
    <p>fame <%= @fame %></p>
    <%= button_to "new video", new_video_path, :method => "get" %>
  </div>
</div>
<h2>HOME.</h2>

<%= render @videos %>


