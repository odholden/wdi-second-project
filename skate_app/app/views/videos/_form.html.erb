<div class="formblock">
  <%= form_for(@video) do |f| %>
    <% if @video.errors.any? %>
      <div id="error_explanation">
        <h2><%= pluralize(@video.errors.count, "error") %> prohibited this video from being saved:</h2>

        <ul>
        <% @video.errors.full_messages.each do |message| %>
          <li><%= message %></li>
        <% end %>
        </ul>
      </div>
    <% end %>

    <div class="field">
      <%= f.label :vehicle_id %><br>
      <%= f.collection_select(:vehicle_id, Vehicle.all, :id, :name) %>
    </div>
    <div class="field">
      <%= f.label :city_id %><br>
      <%= f.number_field :city_id %>
    </div>
    <div class="field">
      <%= f.label :title %><br>
      <%= f.text_field :title %>
    </div>
    <div class="field">
      <%= f.label :description %><br>
      <%= f.text_area :description %>
    </div>
    <div class="field">
      <%= f.label :url %><br>
      <%= f.text_field :url %>
    </div>
    <div class="actions">
      <%= f.submit %>
    </div>
  <% end %>
</div>



